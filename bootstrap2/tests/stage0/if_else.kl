fn main():
    let a = 1
    let b = 2
    if a == 1:
        print("a is 1")
    end
    if a == b:
        print("a is b")
    else:
        print("a is not b")
    end
    if a == 1 and b == 2:
        print("a is 1 and b is 2")
    end
    if a == 2 or b == 2:
        print("a is 2 or b is 2")
    end
    -- if / else if / else
    if a == b => print("a is b")
    else => if a == 1 => print("a is 1")
    else => print("a is not 1")
end
