struct Point:
    x i32
    y i32
end

impl Point:
    -- This is a static method.
    fn new(x i32, y i32) Point:
        return Point{ x: x, y: y }
    end

    fn print_members(self):
        print(self.x)
        print(self.y)
    end

    fn add_to_y(mut self, value i32):
        self.y = self.y + value
    end
end

struct Line:
    start Point
    end_ Point
end

impl Line:
    fn get_start(self) Point:
        return self.start
    end
end

fn simple():
    let p = Point{ x: 1, y: 2 }
    p.print_members()
end

fn mutable():
    mut mp = Point{ x: 3, y: 4 }
    mp.x = 5
    mp.add_to_y(2)
    print(mp.x)
    print(mp.y)
end

fn nested():
    mut line = Line{ 
        start: Point{ x: 10, y: 11 }, 
        end_: Point{ x: 12, y: 13 } 
    }
    line.end_.x = 16
    line.end_.y = line.end_.x + 1
    line.start.print_members()
    line.end_.print_members()
end

fn chained_field_access():
    let line = Line{ 
        start: Point{ x: 20, y: 21 }, 
        end_: Point{ x: 22, y: 23 } 
    }
    line.get_start().print_members()
    print(line.get_start().x)
end

fn static_method():
    let p = Point.new(30, 31)
    p.print_members()
end


fn main():
    simple()
    mutable()
    nested()
    chained_field_access()
    static_method()
    return 0
end
