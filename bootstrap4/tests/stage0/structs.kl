struct Point:
    x i32
    y i32
end

impl Point:
    fn print_members(self):
        print(self.x)
        print(self.y)
    end

    fn add_to_y(mut self, value i32):
        self.y = self.y + value
    end
end

struct Line:
    start Point
    end_ Point
end

fn simple():
    let p = Point{ x: 1, y: 2 }
    p.print_members()
end

fn mutable():
    mut mp = Point{ x: 3, y: 4 }
    mp.x = 5
    mp.add_to_y(2)
    print(mp.x)
    print(mp.y)
end

fn nested():
    mut line = Line{ 
        start: Point{ x: 10, y: 11 }, 
        end_: Point{ x: 12, y: 13 } 
    }
    line.end_.x = 16
    line.end_.y = line.end_.x + 1
    line.start.print_members()
    line.end_.print_members()
end


fn main():
    simple()
    mutable()
    nested()
    return 0
end
